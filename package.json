{
  "name": "regxen",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "prepare": "lefthook install",
    "vitest": "vitest -c ./config/vitest/vitest.config.ts",
    "test:unit": "pnpm vitest --project unit",
    "test:integration": "pnpm vitest --project integration",
    "test:smoke": "pnpm test:integration test/RegXen.generate.test.ts",
    "test:all": "pnpm vitest --project all run --coverage --coverage.include 'src/**/*.ts'",
    "build": "tsc -p ./config/tsconfig/build.json --outDir dist --declaration",
    "types:check": "tsc -p ./config/tsconfig/types.json --noEmit",
    "lint": "biome check --config-path=./config/biome.json",
    "generate:include-list": "pnpm tsx ./config/scripts/generate-reduced-code-set-include-list.ts"
  },
  "files": [
    "./dist"
  ],
  "keywords": [
    "regex",
    "regexp",
    "regular",
    "expression",
    "parser",
    "generator"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/node": "^20.14.12",
    "@vitest/coverage-v8": "^2.0.4",
    "lefthook": "^1.7.11",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4",
    "vitest": "^2.0.4"
  },
  "dependencies": {
    "@eslint-community/regexpp": "^4.11.0"
  }
}
